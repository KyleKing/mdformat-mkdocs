{
    "$schema": "https://raw.githubusercontent.com/hougesen/mdsf/main/schemas/v0.11.0/mdsf.schema.json",
    "languages": {
        "go": "gofmt",
        "javascript": [
            [
                "deno:fmt",
                "prettier"
            ]
        ],
        "json": [
            [
                "deno:fmt",
                "prettier"
            ]
        ],
        "python": "ruff:format",
        "shell": [
